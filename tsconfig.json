{
  "compilerOptions": {
    "target": "ES6",
    "module": "ESNext",
    "sourceMap": false,
    "rootDir": "./src",
    "outDir": "./demo_test",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "types": ["node"],
    "baseUrl": ".",
    "paths": {
      "vitest": ["node_modules/vitest"],
      "types/*": ["src/types/*"],
      "reactivity": ["src/reactivity/*"],
      "renderer": ["src/renderer/*"],
      "compiler/*": ["src/compiler/*"]
    }
    //   "lib": ["DOM","ESNext"]
  },
  "include": ["./src/**/*.ts"],
  "exclude": ["node_modules"]
}
